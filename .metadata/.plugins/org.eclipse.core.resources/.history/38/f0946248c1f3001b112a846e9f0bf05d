package br.comgenerationaula01;

public class ExemploVariavel02 {

	public static void main(String[] args) {
		

		int var1;
		double var2;
		
		var1=10;
		var2= 10.0;
		
		System.out.println("Valor original da primeira variável: " +var1);
		System.out.println("Valor original da segunda variável: " +var2);
		
		
	}

}
